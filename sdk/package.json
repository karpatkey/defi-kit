{
  "name": "defi-kit",
  "version": "1.7.1",
  "description": "Permissions for Zodiac Roles covering interactions with DeFi protocols",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./eth": {
      "require": "./dist/eth.js",
      "import": "./dist/eth.mjs",
      "types": "./dist/eth.d.ts"
    },
    "./gno": {
      "require": "./dist/gno.js",
      "import": "./dist/gno.mjs",
      "types": "./dist/gno.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "eth": [
        "./dist/eth.d.ts"
      ],
      "gno": [
        "./dist/gno.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "setup": "eth-sdk",
    "build": "rimraf dist && tsup --format esm,cjs",
    "dev": "tsup --format esm --watch",
    "anvil": "anvil --silent --fork-url https://rpc.ankr.com/eth",
    "test:watch-only": "jest --runInBand --watch",
    "test": "concurrently --raw --kill-others --success first \"yarn anvil\" \"jest --runInBand\"",
    "test:watch": "concurrently --raw --kill-others --handle-input --passthrough-arguments \"yarn anvil\" \"jest --runInBand --watch {@}\" --",
    "check:types": "tsc --noEmit"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^5.5.0",
    "@ethersproject/abi": "^5.7.0",
    "ethers": "^5.7.2",
    "zod": "^3.22.2",
    "zodiac-roles-sdk": "^2.5.1"
  },
  "devDependencies": {
    "@dethcrypto/eth-sdk": "^0.3.4",
    "@dethcrypto/eth-sdk-client": "^0.1.6",
    "@gnosis.pm/zodiac": "^3.5.2",
    "@types/jest": "^29.5.2",
    "@types/wait-on": "^5.3.1",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "wait-on": "^7.0.1"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
